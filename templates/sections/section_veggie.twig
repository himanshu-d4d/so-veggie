 <section class="veggie">
      <div class="container">
        <div class="row">
          <div class="col-sm-12">
            <h2 class="head">{{entry.veggieHeading}}</h2>
          </div>
          {% for veggie in entry.veggies.all() %}
          {% set image = veggie.veggieImage.one() %}
          <div class="col-sm-6">
            <div class="cont-box">
              <div class="img">
                <img src="{{ image.getUrl() }}" alt="image-left" class="img-right">
              </div>
              <h3>{{veggie.veggieName}}</h3>
              <p>{{veggie.veggieDescription}}</p>
            </div>
          </div>
          {% endfor %}
        </div>
      </div>
    </section>